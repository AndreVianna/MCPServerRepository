    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[markdown]": {
    "editor.defaultFormatter": "vscode.markdown-language-features",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "on",
      "strings": "on"
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[dockerfile]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[sql]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[powershell]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[shellscript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  // Testing settings
  "dotnetCoreExplorer.enableLogging": true,
  "dotnetCoreExplorer.logLevel": "Info",
  "testExplorer.useNativeTesting": true,
  "testExplorer.showOnRun": true,
  "testExplorer.onStart": "reset",
  "testExplorer.onReload": "reset",
  "testExplorer.mergeSuites": true,
  "testExplorer.sort": "byLocation",
  "testExplorer.showExpandButton": true,
  "testExplorer.showCollapseButton": true,
  "testExplorer.gutterDecoration": true,
  "testExplorer.errorDecoration": true,
  "testExplorer.codeLens": true,
  // Git settings
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.autofetchPeriod": 180,
  "git.enableStatusBarSync": true,
  "git.showProgress": true,
  "git.untrackedChanges": "separate",
  "git.fetchOnPull": true,
  "git.pruneOnFetch": true,
  "git.pullBeforeCheckout": true,
  "git.showInlineOpenFileAction": true,
  "git.openRepositoryInParentFolders": "always",
  "git.enableCommitSigning": false,
  "git.rebaseWhenSync": true,
  "git.timeline.date": "committed",
  "git.timeline.showAuthor": true,
  "git.decorations.enabled": true,
  "git.branchProtection": true,
  "git.mergeEditor": false,
  "git.repositoryScanMaxDepth": 2,
  "git.scanRepositories": [
    "/workspace"
  ],
  // Terminal settings
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.fontFamily": "Consolas, 'Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.confirmOnExit": "never",
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.env.linux": {
    "DOTNET_CLI_TELEMETRY_OPTOUT": "1",
    "DOTNET_NOLOGO": "1",
    "DOTNET_SKIP_FIRST_TIME_EXPERIENCE": "1",
    "NUGET_PACKAGES": "/home/vscode/.nuget/packages"
  },
  "terminal.integrated.cwd": "/workspace",
  // Docker settings
  "docker.showStartPage": false,
  "docker.images.sortBy": "CreatedTime",
  "docker.images.groupBy": "Repository",
  "docker.containers.sortBy": "CreatedTime",
  "docker.containers.groupBy": "None",
  "docker.commands.build": "docker build --rm -f \"${dockerfile}\" -t ${tag} \"${context}\"",
  "docker.commands.run": "docker run --rm -d ${tag}",
  "docker.commands.runInteractive": "docker run --rm -it ${tag}",
  // Playwright settings
  "playwright.showTrace": true,
  "playwright.reuseBrowser": true,
  "playwright.showReport": true,
  // GitHub Copilot settings
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": true,
    "csharp": true,
    "javascript": true,
    "typescript": true,
    "json": true,
    "jsonc": true,
    "html": true,
    "css": true,
    "scss": true,
    "dockerfile": true,
    "sql": true,
    "powershell": true,
    "shellscript": true
  },
  "github.copilot.editor.enableAutoCompletions": true,
  // Debugging settings
  "debug.allowBreakpointsEverywhere": false,
  "debug.openDebug": "openOnDebugBreak",
  "debug.inlineValues": "on",
  "debug.toolBarLocation": "docked",
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.showInlineBreakpointCandidates": true,
  "debug.console.acceptSuggestionOnEnter": "on",
  "debug.console.fontSize": 14,
  "debug.console.fontFamily": "Consolas, 'Courier New', monospace",
