{
  // General editor settings
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.trimAutoWhitespace": true,
  "editor.rulers": [
    120,
    140
  ],
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.minimap.maxColumn": 120,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.removeUnusedImports": "explicit"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.inlayHints.enabled": "on",
  "editor.linkedEditing": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggest.preview": true,
  "editor.parameterHints.enabled": true,
  "editor.quickSuggestions": {
    "strings": "on",
    "comments": "on",
    "other": "on"
  },
  // File settings
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.hotExit": "onExit",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "files.exclude": {
    "**/bin": true,
    "**/obj": true,
    "**/.vs": true,
    "**/node_modules": true,
    "**/coverage": true,
    "**/TestResults": true,
    "**/*.user": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "files.watcherExclude": {
    "**/bin/**": true,
    "**/obj/**": true,
    "**/.vs/**": true,
    "**/node_modules/**": true,
    "**/coverage/**": true,
    "**/TestResults/**": true,
    "**/.git/**": true
  },
  // Search settings
  "search.exclude": {
    "**/bin": true,
    "**/obj": true,
    "**/.vs": true,
    "**/node_modules": true,
    "**/coverage": true,
    "**/TestResults": true,
    "**/*.min.js": true,
    "**/*.min.css": true,
    "**/*.js.map": true,
    "**/*.css.map": true
  },
  "search.useGlobalIgnoreFiles": true,
  "search.useIgnoreFiles": true,
  // .NET and C# specific settings
  "dotnet.defaultSolution": "Source/MCPHub.sln",
  "dotnet.server.useOmnisharp": false,
  "dotnet.inlayHints.enableInlayHintsForParameters": true,
  "dotnet.inlayHints.enableInlayHintsForLiteralParameters": true,
  "dotnet.inlayHints.enableInlayHintsForIndexerParameters": true,
  "dotnet.inlayHints.enableInlayHintsForObjectCreationParameters": true,
  "dotnet.inlayHints.enableInlayHintsForOtherParameters": true,
  "dotnet.inlayHints.enableInlayHintsForTypes": true,
  "dotnet.inlayHints.enableInlayHintsForImplicitVariableTypes": true,
  "dotnet.inlayHints.enableInlayHintsForLambdaParameterTypes": true,
  "dotnet.inlayHints.enableInlayHintsForImplicitObjectCreation": true,
  "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
  "dotnet.symbolSearch.searchReferenceAssemblies": true,
  "dotnet.navigation.navigateToDecompiledSources": true,
  "dotnet.projects.enableAutomaticRestore": true,
  "dotnet.backgroundAnalysis.analyzerDiagnosticsScope": "openFiles",
  "dotnet.backgroundAnalysis.compilerDiagnosticsScope": "openFiles",
  // OmniSharp legacy settings (if needed)
  "omnisharp.useModernNet": true,
  "omnisharp.enableEditorConfigSupport": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.enableAsyncCompletion": true,
  "omnisharp.analyzeOpenDocumentsOnly": true,
  "omnisharp.maxProjectResults": 250,
  "omnisharp.defaultLaunchSolution": "Source/MCPHub.sln",
  // Language specific settings
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "on",
      "strings": "on"
